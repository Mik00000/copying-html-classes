{
  "name": "extract-scss-classes",
  "displayName": "Extract SCSS Classes",
  "publisher": "Mrhopro",
  "description": "Extension for copying SCSS blocks from HTML/JSX classes with nesting in mind.",
  "version": "0.0.9",
  "engines": {
    "vscode": "^1.70.0"
  },
  "icon": "icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:extension.extractScss"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.extractScss",
        "title": "Extract SCSS from HTML/JSX",
        "category": "SCSS Tools"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "@babel/parser": "^7.27.0",
    "@babel/traverse": "^7.27.0"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.70.0",
    "eslint": "^9.0.0",
    "@vscode/test-electron": "^2.5.2"
  }
}
